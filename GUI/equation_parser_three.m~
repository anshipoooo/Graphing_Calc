function equation_parser_three()

global funcParse

%% Convert user input into function --> previously Graphing_Calc
% gvar.raw_in=input('Enter your function in terms of x: ','s');    
%% Implicit multiplication

    funcParse.format_in=strcat(funcParse.raw_in,'+1-1');
TF=isstrprop(funcParse.format_in,'digit');
LF=isstrprop(funcParse.format_in,'alpha');
for implicit_loop=1:length(TF)-1
    TF=isstrprop(funcParse.format_in,'digit');
    if TF(1,implicit_loop)==1 && funcParse.format_in(:,implicit_loop+1)=='x'
        funcParse.implicit_before_dig=extractBefore(funcParse.format_in,implicit_loop+1);
        funcParse.implicit_after_dig=extractAfter(funcParse.format_in,implicit_loop);
        funcParse.format_in=strcat(funcParse.implicit_before_dig,'.*',funcParse.implicit_after_dig);
    end
    LF=isstrprop(funcParse.format_in,'alpha');
    if funcParse.format_in(:,implicit_loop)=='x' && LF(:,implicit_loop+1)==1
        funcParse.implicit_before_alpha=extractBefore(funcParse.format_in,implicit_loop+1);
        funcParse.implicit_after_alpha=extractAfter(funcParse.format_in,implicit_loop);
        funcParse.format_in=strcat(funcParse.implicit_before_alpha,'.*',funcParse.implicit_after_alpha);
    end
    if funcParse.format_in(:,implicit_loop)=='y' && LF(:,implicit_loop+1)==1
        funcParse.implicit_before_alpha=extractBefore(funcParse.format_in,implicit_loop+1);
        funcParse.implicit_after_alpha=extractAfter(funcParse.format_in,implicit_loop);
        funcParse.format_in=strcat(funcParse.implicit_before_alpha,'.*',funcParse.implicit_after_alpha);
    end
    if funcParse.format_in(:,implicit_loop)=='^' && LF(:,implicit_loop+1)==1
        funcParse.implicit_before_exp=extractBefore(funcParse.format_in,implicit_loop+1);
        funcParse.implicit_after_exp=extractAfter(funcParse.format_in,implicit_loop);
        funcParse.format_in=strcat(funcParse.implicit_before_exp,'(',funcParse.implicit_after_exp,')');
    end
    

end
funcParse.format_in=strrep(funcParse.format_in,'+1-1','');

%% Square root function
funcParse.format_in=strrep(funcParse.format_in,'?(','sqrt(');

%% Basic switches
     
     funcParse.format_in=strrep(funcParse.format_in,'/','./');
%      gvar.format_in=strrep(gvar.format_in,'*','.*');
     funcParse.format_in=strrep(funcParse.format_in,'+-','-');
     funcParse.format_in=strrep(funcParse.format_in,')(',').*(');
     funcParse.format_in=strrep(funcParse.format_in,'ln(','log(');
     funcParse.format_in=strrep(funcParse.format_in,'arcsin(','asin(');
     funcParse.format_in=strrep(funcParse.format_in,'arccos(','acos(');
     funcParse.format_in=strrep(funcParse.format_in,'arctan(','atan(');
     funcParse.format_in=strrep(funcParse.format_in,'arccsc(','acsc(');
     funcParse.format_in=strrep(funcParse.format_in,'arcsec(','asec(');
     funcParse.format_in=strrep(funcParse.format_in,'arccot(','acot(');

 %% Exponential functions
 if contains(funcParse.format_in,'e^(') 
     
     funcParse.format_in=strrep(funcParse.format_in,'e^(','exp(');
 elseif contains(funcParse.format_in,'e.^(')
     funcParse.format_in=strrep(funcParse.format_in,'e.^(','exp(');

 
 elseif contains(funcParse.raw_in,'e^x')
     funcParse.format_in=strrep(funcParse.format_in,'e^x','exp(x)');
     
 elseif contains(funcParse.raw_in,'e^y')
     funcParse.format_in=strrep(fu
     
 elseif contains(funcParse.raw_in,'^x') || contains(funcParse.raw_in,'^(')...
         || contains(funcParse.raw_in,'^y')

     funcParse.format_in=strrep(funcParse.format_in,'^','.^');
 elseif contains(funcParse.raw_in,'x^') 
     funcParse.format_in=strrep(funcParse.format_in,'x^','x.^');
 elseif contains(funcParse.raw_in,'y^')
     funcParse.format_in=strrep(funcParse.format_in,'y^','y.^');
 end



 %% Absolute value change

if contains(funcParse.raw_in,'|')
    abs_split=strsplit(funcParse.format_in,'|');
    abs_in=strcat('abs(',abs_split{2},')');
    funcParse.format_in=strcat(abs_split{1},abs_in,abs_split{3});

end



end

