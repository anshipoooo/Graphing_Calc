function relMinMax()

global gvar
%% Relative min/max
gvar.RMM_x=cell(1,length(gvar.real_sec)-1);
gvar.RMM_y=cell(1,length(gvar.real_sec)-1);
for rel_min=2:length(gvar.real_sec)-1
    if gvar.real_first(:,rel_min-1)>0 && gvar.real_first(:,rel_min+1)<0
        
        
        gvar.RMM_x{1,rel_min}=round(gvar.real_orig_x(:,rel_min),3);
        gvar.RMM_x=gvar.RMM_x(~cellfun('isempty',gvar.RMM_x));
        gvar.RMM_y{1,rel_min}=round(gvar.real_orig_y(:,rel_min),3);
        gvar.RMM_y=gvar.RMM_y(~cellfun('isempty',gvar.RMM_y));
        
            
    
    elseif gvar.real_first(:,rel_min-1)<0 && gvar.real_first(:,rel_min+1)>0
        
        gvar.RMM_x{1,rel_min}=round(gvar.real_orig_x(:,rel_min),3);
        gvar.RMM_x=gvar.RMM_x(~cellfun('isempty',gvar.RMM_x));
        gvar.RMM_y{1,rel_min}=round(gvar.real_orig_y(:,rel_min),3);
        gvar.RMM_y=gvar.RMM_y(~cellfun('isempty',gvar.RMM_y));
        
            
    end

end

for relate=2:length(gvar.RMM_y)
    if gvar.RMM_x{1,relate}==gvar.RMM_x{1,relate-1}
        gvar.RMM_x{1,relate}=[];
        gvar.RMM_y{1,relate}=[];
    end

end
gvar.RMM_x=gvar.RMM_x(~cellfun('isempty',gvar.RMM_x));
gvar.RMM_y=gvar.RMM_y(~cellfun('isempty',gvar.RMM_y));
gvar.RMM_str=cell(1,length(gvar.RMM_x));
for str=1:length(gvar.RMM_y)
    gvar.RMM_x{1,str}=num2str(gvar.RMM_x{1,str});
    gvar.RMM_y{1,str}=num2str(gvar.RMM_y{1,str});
    gvar.RMM_str{1,str}=strcat('(',gvar.RMM_x{1,str},',',gvar.RMM_y{1,str},')');
end


%%%%%%%%%%


loc=zeros(1,length(gvar.real_sec)-1);
for i=2:length(gvar.real_sec)-1
    if gvar.real_sec(:,i-1)>0 && gvar.real_sec(:,i+1)<0
        
        loc(:,i)=i;
        
        
    end
end

loc(loc==0)=[];

RMM_x_matr=zeros(1,length(loc));
RMM_y_matr=zeros(1,length(loc));

for g=1:length(loc)
    RMM_x_matr(:,g)=round(gvar.real_orig_x(loc(:,g)),3);
    RMM_y_matr(:,g)=round(gvar.real_orig_y(loc(:,g)),3);
end


gvar.POI_final_x=RMM_x_matr;
gvar.POI_final_y=RMM_y_matr;
new_location=length(gvar.POI_final_x)-1;
for i=new_location:-1:1
    if RMM_x_matr(:,i)==RMM_x_matr(:,i+1)
        gvar.POI_final_x(i)=[];
        gvar.POI_final_y(i)=[];
    end
end
gvar.POI_x=cell(1,length(gvar.POI_final_x));
gvar.POI_y=cell(1,length(gvar.POI_final_y));
gvar.POI_str=cell(1,length(gvar.POI_final_x));
for i=1:length(gvar.POI_final_x)
    
    gvar.POI_x{1,i}=num2str(gvar.POI_final_x(:,i));
    gvar.POI_y{1,i}=num2str(gvar.POI_final_y(:,i));
    gvar.POI_str{1,i}=strcat('(',gvar.POI_x{1,i},',',gvar.POI_y{1,i},')');
    
end




